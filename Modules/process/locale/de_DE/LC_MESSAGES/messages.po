msgid " * source feed"
msgstr " * Quell Feed"

#: ../../../process_processlist.php:155
msgid "muliply value with source feed"
msgstr "Multipliziert den Wert mit Quell-Feed."

#: ../../../process_processlist.php:156
msgid " / source feed"
msgstr " / Quell Feed"

#: ../../../process_processlist.php:156
msgid "divide value by source feed"
msgstr "Dividiert Wert durch Quell Feed"

